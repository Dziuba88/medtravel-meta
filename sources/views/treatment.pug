  section.mt__treatment--services
    .container
      .container
        h2 Наши услуги
        .super__align
          .services__list
            a.service(href="service.html")
              img.icon(src="img/s1.png", alt="")
              h4 Зубные импланты
            a.service(href="service.html")
              img.icon(src="img/s2.png", alt="")
              h4 Диагностика зубов
            a.service(href="service.html")
              img.icon(src="img/s3.png", alt="")
              h4 Лечение зубов
            a.service(href="service.html")
              img.icon(src="img/s4.png", alt="")
